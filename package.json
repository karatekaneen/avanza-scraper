		"ts-node": "^8.10.1",
		"typescript": "^3.8.3"
	},
	"dependencies": {
		"@google-cloud/firestore": "^2.4.0",
		"dotenv": "^8.2.0",
		"node-fetch": "^2.6.0",
		"puppeteer": "^3.0.4"
	}
}
