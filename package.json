	},
	"homepage": "https://github.com/karatekaneen/avanza-scraper#readme",
	"devDependencies": {
		"@types/jest": "^25.2.1",
		"@types/node": "^13.13.5",
		"@types/node-fetch": "^2.5.7",
		"@types/puppeteer": "^2.0.1",
		"@typescript-eslint/eslint-plugin": "^2.31.0",
		"@typescript-eslint/parser": "^2.31.0",
		"babel-eslint": "^10.1.0",
		"eslint": "^6.8.0",
		"jest": "^26.0.1",
		"nodemon": "^2.0.3",
		"prettier": "^2.0.5",
