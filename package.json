{
	"name": "avanza-scraper",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"scripts": {
		"start": "node index.js",
		"build": "npm run build-ts",
		"debug": "ts-node src/index.ts --inspect=5858",
		"build-ts": "tsc",
		"postbuild": "cp package.json dist/package.json && cp package-lock.json dist/package-lock.json  &&  cd dist && npm ci --only=production",
		"test": "jest --silent --verbose",
		"testwatch": "jest --watchAll",
		"testwatch:silent": "npm run testwatch -- --silent",
		"dev": "nodemon --inspect=5858 -e ts --exec node -r ts-node/register ./src/index.ts",
		"serve": "node dist/index.js"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/karatekaneen/avanza-scraper.git"
